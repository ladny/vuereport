/*确认家访*/
.pawnhead{
	position: relative;
	height:0.533333rem;
	background:#0ea3f0 ;
	color: #ffffff;
}
.pawnheadline{
	position: absolute;
	top: 0.533333rem;
	height: 1px;
	width: 100%;
}

.pawnheadline:after { 
    position: absolute; 
    content: ''; 
    width: 100%; 
    left: 0; 
    bottom: 0; 
    height: 1px; 
    background-color: #ffffff; 
    -webkit-transform: scale(1,0.5); 
    transform: scale(1,0.5); 
    -webkit-transform-origin: center bottom; 
    transform-origin: center bottom 
} 
.pawnheadlist{
	
}
.pawnheadlist a{
	position: relative;
    font-size: 0.346667rem;
    color: #ffffff;
    width: 2.133333rem;
    height: 1.333333rem;
    display: inline-block;
}
.pawnheadlist a span{
	position: absolute;
	top:0.933333rem;
	left: 20%;
}

.pawnheadlist a span.on{
	position: absolute;
	top:0.933333rem;
	left: 27%;
}
.pawnheadlist a span.left2{
	left: 47%;
}

.pawnheadlist a span.leftem{
	left: 40%;
}

.pawnheadlist a em.ok{
	position: absolute;
	top:0.4rem;
	width: 0.32rem;
	height: 0.32rem;
	background: #ffffff;
	border-radius: 50%;
}
.pawnheadlist a em.no{
	position: absolute;
	top:0.4rem;
	width: 0.32rem;
	height: 0.32rem;
	background: #0ea3f0;
	border: 2px solid #5cc1f5;
	border-radius: 50%;
}
.pawnheadicon{
	position: absolute;
	top: 0.293333rem;
	left: 50%;
	width: 0.586667rem;
	height: 0.586667rem;
}
